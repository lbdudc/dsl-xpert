{
  "name": "@lbdudc/dsl-xpert",
  "version": "0.1.1",
  "description": "Sensor LLM DSL Builder",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build:client": "cd src/client && vite build",
    "dev:client": "cd src/client && vite",
    "dev:serve": "node ./src/server/index.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:serve\"",
    "lint_eslint": "eslint --ext js,vue --fix src test",
    "lint_prettier": "prettier --write --ignore-unknown src test",
    "lint": "npm-run-all lint_eslint lint_prettier",
    "prepare": "husky install",
    "prod:client": "cd src/client && vite --host",
    "prod": "concurrently \"npm run prod:client\" \"npm run dev:serve\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "LLM",
    "DSL"
  ],
  "author": "Victor Lamas",
  "license": "MIT",
  "lint-staged": {
    "*.{json,md,yml}": "prettier --write",
    "*.{js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@mdi/font": "^7.1.96",
    "@mdi/js": "^7.0.96",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.4.5",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8"
  },
  "dependencies": {
    "@codingame/monaco-vscode-configuration-service-override": "~10.1.4",
    "@codingame/monaco-vscode-keybindings-service-override": "~10.1.4",
    "@codingame/monaco-vscode-standalone-languages": "~10.1.4",
    "@mlc-ai/web-llm": "^0.2.77",
    "@primevue/auto-import-resolver": "^4.2.1",
    "@primevue/forms": "^4.2.1",
    "@primevue/themes": "^4.2.1",
    "@vitejs/plugin-vue": "^4.5.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "~4.21.1",
    "langium": "~3.2.0",
    "monaco-editor": "npm:@codingame/monaco-vscode-editor-api@~10.1.4",
    "monaco-editor-wrapper": "~6.0.0-next.6",
    "monaco-languageclient": "~9.0.0-next.6",
    "mongoose": "^8.3.1",
    "npm-run-all": "^4.1.5",
    "openai": "^4.37.1",
    "postcss": "^8.4.20",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.1",
    "tailwindcss": "^3.2.4",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.0.11",
    "vscode": "npm:@codingame/monaco-vscode-api@~10.1.4",
    "vscode-json-languageservice": "~5.4.1",
    "vscode-languageclient": "~9.0.1",
    "vscode-languageserver": "~9.0.1",
    "vscode-languageserver-protocol": "~3.17.5",
    "vscode-uri": "~3.0.8",
    "vue": "^3.2.41",
    "vue-router": "^4.2.5"
  }
}